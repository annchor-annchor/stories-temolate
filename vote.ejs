<%- include("partials/header") -%>

<div class="wrapper">

   <div class="box1" style="
    grid-column-start: 1;
    grid-column-end: 2;
    grid-row-start: 1;
    grid-row-end: 3;
    ">
		<div class="profile dyn" style="margin-top: 50%;">
			<img class="avatar" src="https://github.com/yndx-shri/shri-2021-task-1/blob/master/assets/images/1x/<%= data.users[0].avatar%>?raw=true" alt="avatar-img">
			<p class="name"><%= users[0].name%></p>
		</div>
	</div>
   <div class="box2" style="
    grid-column-start: 2;
    grid-column-end: 3;
    grid-row-start: 1;
    grid-row-end: 2;
    ">
		<div class="profile dyn">
			<img class="avatar" src="https://github.com/yndx-shri/shri-2021-task-1/blob/master/assets/images/1x/<%= data.users[1].avatar%>?raw=true" alt="avatar-img">
			<p class="name"><%= users[1].name%></p>
		</div>
	</div>
   	<div class="box3" style="
   	grid-column-start: 2;
    grid-column-end: 3;
    grid-row-start: 2;
    grid-row-end: 3;
   ">		
   		<div class="profile dyn">
			<img class="avatar" src="https://github.com/yndx-shri/shri-2021-task-1/blob/master/assets/images/1x/<%= data.users[2].avatar%>?raw=true" alt="avatar-img">
			<p class="name"><%= users[2].name%></p>
		</div>
	</div>
   <div class="box4" style="
